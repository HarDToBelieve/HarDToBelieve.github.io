<dec f='webkit/Source/ThirdParty/libwebrtc/Source/webrtc/rtc_base/event.h' l='45' type='bool rtc::Event::Wait(int give_up_after_ms, int warn_after_ms)'/>
<use f='webkit/Source/ThirdParty/libwebrtc/Source/webrtc/rtc_base/event.h' l='49' u='c' c='_ZN3rtc5Event4WaitEi'/>
<doc f='webkit/Source/ThirdParty/libwebrtc/Source/webrtc/rtc_base/event.h' l='37'>// Waits for the event to become signaled, but logs a warning if it takes more
  // than `warn_after_ms` milliseconds, and gives up completely if it takes more
  // than `give_up_after_ms` milliseconds. (If `warn_after_ms &gt;=
  // give_up_after_ms`, no warning will be logged.) Either or both may be
  // `kForever`, which means wait indefinitely.
  //
  // Returns true if the event was signaled, false if there was a timeout or
  // some other error.</doc>
<def f='webkit/Source/ThirdParty/libwebrtc/Source/webrtc/rtc_base/event.cc' l='139' ll='199' type='bool rtc::Event::Wait(const int give_up_after_ms, const int warn_after_ms)'/>
