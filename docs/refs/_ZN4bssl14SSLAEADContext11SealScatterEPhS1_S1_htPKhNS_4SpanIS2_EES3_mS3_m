<dec f='webkit/Source/ThirdParty/libwebrtc/Source/third_party/boringssl/src/ssl/internal.h' l='853' type='bool bssl::SSLAEADContext::SealScatter(uint8_t * out_prefix, uint8_t * out, uint8_t * out_suffix, uint8_t type, uint16_t record_version, const uint8_t * seqnum, Span&lt;const uint8_t&gt; header, const uint8_t * in, size_t in_len, const uint8_t * extra_in, size_t extra_in_len)'/>
<doc f='webkit/Source/ThirdParty/libwebrtc/Source/third_party/boringssl/src/ssl/internal.h' l='838'>// SealScatter encrypts and authenticates |in_len| bytes from |in| and splits
  // the result between |out_prefix|, |out| and |out_suffix|. It returns one on
  // success and zero on error.
  //
  // On successful return, exactly |ExplicitNonceLen| bytes are written to
  // |out_prefix|, |in_len| bytes to |out|, and |SuffixLen| bytes to
  // |out_suffix|.
  //
  // |extra_in| may point to an additional plaintext buffer. If present,
  // |extra_in_len| additional bytes are encrypted and authenticated, and the
  // ciphertext is written to the beginning of |out_suffix|. |SuffixLen| should
  // be used to size |out_suffix| accordingly.
  //
  // If |in| and |out| alias then |out| must be == |in|. Other arguments may not
  // alias anything.</doc>
<def f='webkit/Source/ThirdParty/libwebrtc/Source/third_party/boringssl/src/ssl/ssl_aead_ctx.cc' l='314' ll='397' type='bool bssl::SSLAEADContext::SealScatter(uint8_t * out_prefix, uint8_t * out, uint8_t * out_suffix, uint8_t type, uint16_t record_version, const uint8_t * seqnum, Span&lt;const uint8_t&gt; header, const uint8_t * in, size_t in_len, const uint8_t * extra_in, size_t extra_in_len)'/>
