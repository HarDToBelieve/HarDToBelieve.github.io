<def f='webkit/Source/ThirdParty/libwebrtc/Source/webrtc/media/sctp/sctp_transport_internal.h' l='46' type='const uint16_t'/>
<use f='webkit/Source/ThirdParty/libwebrtc/Source/webrtc/media/sctp/sctp_transport_internal.h' l='47' u='r'/>
<doc f='webkit/Source/ThirdParty/libwebrtc/Source/webrtc/media/sctp/sctp_transport_internal.h' l='36'>// The number of outgoing streams that we&apos;ll negotiate. Since stream IDs (SIDs)
// are 0-based, the highest usable SID is 1023.
//
// It&apos;s recommended to use the maximum of 65535 in:
// https://tools.ietf.org/html/draft-ietf-rtcweb-data-channel-13#section-6.2
// However, we use 1024 in order to save memory. usrsctp allocates 104 bytes
// for each pair of incoming/outgoing streams (on a 64-bit system), so 65535
// streams would waste ~6MB.
//
// Note: &quot;max&quot; and &quot;min&quot; here are inclusive.</doc>
<use f='webkit/Source/ThirdParty/libwebrtc/Source/webrtc/media/sctp/usrsctp_transport.cc' l='368' u='r' c='_ZN7cricket16UsrsctpTransport14UsrSctpWrapper17InitializeUsrSctpEv'/>
<use f='webkit/Source/ThirdParty/libwebrtc/Source/webrtc/pc/webrtc_sdp.cc' l='1355' u='r' c='_ZN6webrtcL26BuildSctpContentAttributesEPNSt3__112basic_stringIcNS0_11char_traitsIcEENS0_9allocatorIcEEEEPKN7cricket26SctpDataContentDescriptionE'/>
<def f='webkit/WebKitBuild/Debug/usr/local/include/webrtc/media/sctp/sctp_transport_internal.h' l='46' type='const uint16_t'/>
<use f='webkit/WebKitBuild/Debug/usr/local/include/webrtc/media/sctp/sctp_transport_internal.h' l='47' u='r'/>
