<doc f='webkit/Source/ThirdParty/ANGLE/src/compiler/translator/TranslatorMetalDirect/RewriteCaseDeclarations.h' l='16'>// EXAMPLE
//    switch (expr)
//    {
//      case 0:
//        int x = 0;
//        break;
//      case 1:
//        int y = 0;
//        {
//          int z = 0;
//        }
//        break;
//    }
// Becomes
//    {
//      int x;
//      int y;
//      switch (expr)
//      {
//        case 0:
//          x = 0;
//          break;
//        case 1:
//          y = 0;
//          {
//            int z = 0;
//          }
//          break;
//      }
//    }</doc>
